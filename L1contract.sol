interface RaffleL2 {
    function createRaffle(
        string memory domainName,
        uint256 minimumAmount,
        uint256 deadline,
        uint256 prize,
        uint256 totalTickets,
        address domainOwner
    ) external;
}

contract RaffleL1 {
    uint256 public count = 1;
    uint256 public prova2;

    function prova() public {
        count++;
    }

    address public l2Target;
    IInbox public inbox;

    event RetryableTicketCreated(uint256 indexed ticketId);

    constructor(address _l2Target, address _inbox) {
        l2Target = _l2Target;
        inbox = IInbox(_inbox);
    }

    function updateL2Target(address _l2Target) public {
        l2Target = _l2Target;
    }

    struct Winner {
        uint256 tokenId; // ID of the token
        address winner; // Address of the winning Raffle
    }
    // Map tokenIds to Winners
    mapping(uint256 => Winner) public winners;

    // Function to create a new Winner of a raffle


    // Function to get a Winner
    function getWinner(uint256 _tokenId)
        public
        view
        returns (
            uint256,
            address
        )
    {
        // Get the Winner
        Winner memory winner = winners[_tokenId];

        // Return the Winner details
        return (winner.tokenId, winner.winner);
    }

       function getPrize(uint256 _tokenId, address _winner) public {
        ExternalContract externalContract = ExternalContract(
            0x57f1887a8BF19b14fC0dF6Fd9B2acc9Af147eA85
        );
        // Create a new Winner
        IBridge bridge = inbox.bridge();
        // this prevents reentrancies on L2 to L1 txs
        require(msg.sender == address(bridge), "NOT_BRIDGE");
        IOutbox outbox = IOutbox(bridge.activeOutbox());
        address l2Sender = outbox.l2ToL1Sender();
        require(l2Sender == l2Target, "claim Prize only execute by L2");

        externalContract.safeTransferFrom(address(this), _winner, _tokenId);
        externalContract.reclaim(_tokenId, _winner);
    }



    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) public returns (bytes4) {
        return this.onERC721Received.selector;
    }


    function setRaffleInL2(
        string memory domainName,
        uint256 minimumAmount,
        uint256 deadline,
        uint256 prize,
        uint256 totalTickets,
        address domainOwner,
        uint256 maxSubmissionCost,
        uint256 maxGas,
        uint256 gasPriceBid
    ) public payable returns (uint256) {
        ExternalContract externalContract = ExternalContract(
            0x57f1887a8BF19b14fC0dF6Fd9B2acc9Af147eA85
        );

        require(
            externalContract.ownerOf(prize) == msg.sender,
            "devi essere l'owner"
        );
        bytes memory data = abi.encodeWithSelector(
            RaffleL2.createRaffle.selector,
            domainName,
            minimumAmount,
            deadline,
            prize,
            totalTickets,
            domainOwner
        );

        uint256 ticketID = inbox.createRetryableTicket{value: msg.value}(
            l2Target,
            0,
            maxSubmissionCost,
            msg.sender,
            msg.sender,
            maxGas,
            gasPriceBid,
            data
        );

        emit RetryableTicketCreated(ticketID);

        externalContract.safeTransferFrom(msg.sender, address(this), prize);
        externalContract.reclaim(prize, address(this));

        return ticketID;
    }
}